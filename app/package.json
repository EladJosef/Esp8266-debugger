{
  "name": "esp8266-debugger",
  "productName": "Esp8266 debugger",
  "version": "1.0.0",
  "description": "Esp8266 debugger",
  "main": "public/main.js",
  "author": {
    "email": "eladyosef2003@gmail.com",
    "name": "elad yosef"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "serve": "sirv public",
    "electron-start": "electronmon ./public/main.js",
    "start": "npm run electron-start",
    "validate": "svelte-check",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "mac": {
    "icon": "./assets/icon/icon.icns"
  },
    "linux": {
    "icon": "./assets/icon/icon.png"
  },
    "win": {
    "icon": "./assets/icon/icon.ico"
  }
},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "electron": "^10.3.0",
    "electron-builder": "^22.9.1",
    "electron-icon-builder": "^2.0.1",
    "electron-packager": "^15.2.0",
    "electronmon": "^1.1.2",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "css-checkbox-library": "^1.5.2",
    "dialog": "^0.3.1",
    "remote": "^0.2.6",
    "rs": "0.0.1",
    "sirv-cli": "^1.0.0"
  }
}
